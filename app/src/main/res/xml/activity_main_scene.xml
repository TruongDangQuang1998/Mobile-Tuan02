<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="300dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="1."
            android:id="@+id/colored_bar" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/text98"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="168dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.695" />
        <Constraint
            motion:layout_editor_absoluteY="574dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="53dp"
            android:layout_width="wrap_content"
            android:id="@+id/texttRating" />
        <Constraint
            motion:layout_editor_absoluteY="574dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="230dp"
            android:layout_width="wrap_content"
            android:id="@+id/textProject" />
        <Constraint
            motion:layout_editor_absoluteY="641dp"
            android:layout_height="57dp"
            android:layout_width="176dp"
            android:id="@+id/button"
            motion:layout_constraintStart_toEndOf="@+id/button2"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:layout_height="57dp"
            android:layout_width="153dp"
            android:id="@+id/button2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="40dp"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="32dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>